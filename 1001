            getInto(id,lessionId,text){
                //开始学习
                var url = 'action.php?a=getStudyPage2&c=LessonProxy'+'&lessonSectionPoint='+id+'&lessionId='+lessionId+"&messageText="+text;
                window.open(url);
            },

