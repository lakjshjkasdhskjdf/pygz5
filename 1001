    function purchase(id){
        //购买课程
        $("#purchaseIfr").attr("src","action.php?c=LessonProxy&a=getSMZF&lessonProductId="+id);
        $("#purchase").show();
    };

    function purchaseClose(){
		$("#purchase").hide();
		$("#purchaseIfr").attr("src","");
	};

