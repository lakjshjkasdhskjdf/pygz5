           if(endY > startY && Math.abs(endY-startY)>20){\r\n\t            \t  index--;\/\/\u5411\u5de6\r\n\t              }\r\n\t              else if(endY < startY && Math.abs(endY-startY)>20){\r\n\t            \t  index++;\/\/\u5411\u53f3\r\n\t              }\r\n\t              else{\r\n\t            \t  isScroll = false;\/\/\u6ca1\u52a8\r\n\t              }\r\n\t              setPageAnimate();\r\n        \t  }\r\n          },false);\r\n          \r\n          \r\n          \/\/\u7a97\u53e3\u5927\u5c0f\u91cd\u7f6e\r\n    \t  window.onresize = function(){\r\n    \t\t  \/\/\u91cd\u65b0\u8bbe\u7f6emainDiv\u7684\u4e0a\u8fb9\u8ddd\r\n    \t\t  var top = document.body.offsetHeight * index * -1;\r\n    \t\t  document.getElementById(\"mainDiv\").style.top = top+\"px\";\r\n    \t  }\r\n          \r\n    \t  \/\/\u53f3\u4fa7\u6d6e\u52a8\u6309\u626d\r\n    \t  for(var i=0;i<=maxIndex;i++){\r\n    \t\t  document.getElementById(\"_btn\"+i).onclick = function(){\r\n    \t\t\t  var id = this.id.replace(\"_btn\",\"\");\r\n    \t\t\t  index = new Number(id);\r\n    \t\t\t  setPageAnimate();\r\n    \t\t  }\r\n          }\r\n    \t  \r\n    \t  \/\/\u94a2\u7434\u5757\uff0c\u6bcf\u4e2ali\u7684\u9f20\u6807\u70b9\u51fb\u4e8b\u4ef6\r\n    \t  for(var i=1;i<=8;i++){\r\n    \t\t  document.getElementById(\"li\"+i).onclick = function(){\r\n    \t\t\t  this.className = \"active\";\r\n    \t\t\t  document.getElementById(\"page2_ul\").className = \"aaa\";\r\n    \t\t\t  \/\/document.getElementsByClassName(\"closeBtn\")[0].style.display = \"block\";\r\n    \t\t  }\r\n    \t  }\r\n    \t  \r\n    \t  \/\/\u94a2\u7434\u5757\uff0c\u5173\u95ed\u6309\u626d\r\n    \t  var closeBtnList = document.getElementsByClassName(\"closeBtn\");\r\n    \t  for(var i=0;i<closeBtnList.length;i++){\r\n\t    \t  closeBtnList[i].onclick = function(e){\r\n\t    \t\t  e.stopPropagation();\/*\u963b\u6b62\u4e8b\u4ef6\u5192\u6ce1(\u70b9\u51fb\u5173\u95ed\u6309\u626d\u65f6\uff0cli\u548cul\u4e5f\u4f1a\u88ab\u70b9\u51fb\u523