            programObjective:0,
        },
        methods: {
            getPoints(str = ""){

                return str.replace(/\n/g, "<br/>");
            },
            getLockHeight(arr){

                var height = 51;
                var total = 0;
                for (let index = 0; index < arr.length; index++) {
                    const section = arr[index];
                    
                    //如果是MAX 题，则没有章节锁
                    if(section.type == 5){

                        return 0;
                    }

                    if( !section.isComplete && !this.isLock([section])){// && section.isTest
                        total++;
                    }
                }

                var result = total * (height + 21)  - 20;
                console.log(result);
                return result;
                
            },
            isLock(arr){
                
                var result = true;

                for (let index = 0; index < arr.length; index++) {
                    const section = arr[index];
                    
                    var toCLock = (section.auth.hasOwnProperty("toCLock")) ? section.auth.toCLock : false;

                    if( !section.auth.status || toCLock ){
                        return result = false;
                    }
                }

                return result;
            },

            isMax(obj){

                for (let index = 0; index < obj.lessonSectionPointArr.length; index++) {
                    const element = obj.lessonSectionPointArr[index];
                    
                    //max 题
                    if(element.type == 5){
                        return "block";
                    }
                }

                return "flex";
            },
            resetWrong(){
                $.ajax({ 
                    url: "action.php", 
                    type:"POST",
                    cache:false,
                    data:{c:"LessonProxy",a:"resetWrongQuestions","userId":"35300","lessonId":lessionId},
                    success: function(json){

                        //统计
                        _getLessonUserData("reset","TEditerProxy");
                        
                        json = JSON.parse(json);

                        if(json.status == 200){

                            vm.$notify({
                                title: '成功',
                                message: '重置成功！',
                                type: 'success'
                            });
                            
                            vm.getWrong();
                        }
                    },
                    error:function(e){
                        console.log(e);
                    }
                });
            },

            showlesson(){
                testRunCode = lesson_code;
                testRunApp();
            },

            getChaptersProgress(chapter){
                //章节进度
                var completeNum = 0;
                var total = 0;
                if(chapter.lessonSectionPointArr.length == 0){
                    return -1;
                }

                for (let index = 0; index < chapter.lessonSectionPointArr.length; index++) {
                    const element = chapter.lessonSectionPointArr[index];
                    if( element.isTest &&  element.testArr.length > 0 ){
                        total++;
                    }
                    if( element.isTest &&  element.testArr.length > 0 && element.isComplete){
                        completeNum++;
                    }

                }

                return (completeNum / total) * 100;
            },

            getClassQuality(chapter){
                //精品章节
                if(chapter.lessonSectionPointArr == undefined){
                    return false;
                }

                for (let index = 0; index < chapter.lessonSectionPointArr.length; index++) {
                    const element = chapter.lessonSectionPointArr[index];
                    
                    if(element.isTest == 1){
                        return true;
                    }
                }

                return false;
            },

            goExtendChallenge(status,event,id1,id2,id3,id4,text){
                if(status == false){
                    return;
                }
                this.knowPoint(event,id1,id2,id3,id4,text);
            },

            goTask(event,taskId){
                //考核任务跳转
                event.stopPropagation();
                
                var url = '../lessonkpi/action.php?c=TaskProxy&a=details&taskId='+window.btoa(taskId)+'&classesId='+window.btoa(this.dataList.classesId)+'&userId=MzUzMDA=';
                window.open(url);
            },

            getScore(evaluateList){
                //考核任务评价
                let score = 3;
                for (let index = 0; index < evaluateList.length; index++) {
                    const element = evaluateList[index];
                    let item = element.item1;
                    if(element.item2 != null){
                        item = element.item2;
                    }

                    if(element.workItemId == 200 && item != 0 ){//如果为态度评价 并且 态度评价项不是优 
                        score =  1;//合格
                        break;
                    }else if(item == 2){ //存在一个差时
                        score = 0;//不合格
                        break;
                    }else if(item <= 1){//全是优良时
                        score =  2;//优秀
                        continue;
                    }else{
                        score =  3;//暂未答题记录
                        continue;
                    }
                }

                return score;
            },
            getWrong(){
                //获取错题回顾
                $.ajax({
                    url: "action.php", 
                    type:"POST",
                    cache:false,
                    data:{c:"LessonProxy",a:"reviewWrongQuestions","userId":"35300","lessonId":lessionId},
                    success: function(json){

                        //统计
                        _getLessonUserData("info","TEditerProxy");

                        if(!json){
                            return;
                        }

                        json = JSON.parse(json);

                        if(json.status == 200){

                            vm.dataList.tediterWrongList = json.tediterList;
                        }
                    },
                    error:function(e){
                        console.log(e);
                    }
                });
            },

            viewLessonSectionCode(num){
                //目标效果
