\u7684\u52a8\u4f5c\r\n\t\t    \r\n\t\t    if(e.wheelDelta){\r\n\t\t    \t\/\/IE\/Opera\/Chrome\r\n\t\t        if(e.wheelDelta < 0){\r\n\t\t        \tindex++;\r\n\t\t        }\r\n\t\t        else{\r\n\t\t        \tindex--;\r\n\t\t        }\r\n\t\t    }\r\n\t\t    else if(e.detail){\r\n\t\t    \t\/\/Firefox\r\n\t\t        if(e.detail < 0){\r\n\t\t        \tindex--;\r\n\t\t        }\r\n\t\t        else{\r\n\t\t        \tindex++;\r\n\t\t        }\r\n\t\t    }\r\n\t\t    setPageAnimate()\r\n\t    }\r\n\t  }\r\n      \r\n      \/\/\u8bbe\u7f6e\u6bcf\u5c4f\u5165\u573a\u52a8\u753b\r\n      function setPageAnimate(){\r\n    \t  if(index < 0){\r\n    \t\t  index = 0;\r\n\t    \t  isScroll = false;\r\n\t      }\r\n\t      if(index > maxIndex){\r\n\t    \t  index = maxIndex;\r\n\t    \t  isScroll = false;\r\n\t      }\r\n\t      \r\n\t      \/\/\u8bbe\u7f6e\u53f3\u4fa7\u7684\u6d6e\u52a8\u6309\u626d\r\n\t\t  for(var i=0;i<=maxIndex;i++){\r\n\t\t\t  document.getElementById(\"_btn\"+i).style.backgroundColor = \"#3D3D3D\";\r\n\t\t  }\r\n\t\t  document.getElementById(\"_btn\"+index).style.backgroundColor = \"#10508D\";\r\n\t\t  \r\n\t\t  \r\n\t\t  \/\/\u8bbe\u7f6e\u6bcf\u5c4f\u7684\u5165\u573a\u52a8\u753b\r\n\t\t  for(var i=0;i<=maxIndex;i++){\r\n\t\t\t  document.getElementById(\"page\"+i).className = \"enter\";\r\n\t\t  }\r\n\t\t  document.getElementById(\"page\"+index).className = \"\";\r\n\t\t  \r\n\t\t  \/\/\u91cd\u7f6e\u7b2c\u4e8c\u5c4f\u7684\u89c6\u9891\r\n\t      if(index != 1){\r\n\t    \t  document.getElementById(\"vo\").pause();\r\n\t    \t  document.getElementById(\"vo\").currentTime = 0;\/\/\u56de\u5230\u5f00\u5934\r\n\t    \t  document.getElementById(\"page1_3\").style.display = \"none\";\r\n\t      }\r\n\t\t  \r\n\t      turnPage();\r\n      }\r\n      \r\n      \/\/\u6eda\u5c4f\u52a8\u753b\r\n      function turnPage(){  \r\n    \t  \r\n          var top = document.body.offsetHeight * index * -1;\r\n          document.getElementById(\"mainDiv\").style.top = top+\"px\";\r\n          \r\n          \/\/\u52a8\u753b\u6548\u679c\u662f\u5426\u5b8c\u6210\r\n          document.getElementById(\"mainDiv\").addEventListener(\"transitionend\",function(){\r\n        \t  isScroll = false;\r\n          });\r\n      }\r\n      \r\n      \/\/\u7b2c\u4e8c\u5c4f\uff0c\u663e\u793a\u89c6\u9891\r\n      function showVideo(){\r\n    \t  document.getElementById(\"page1_3\").style.display = \"block\";\r\n    \t  document.getElementById(\"vo\").play();\r\n    \t  \/\/document.getElementById(\"vo\").pause();\/\/\u505c\u6b62\r\n      }\r\n      \r\n      \/\/\u7b2c\u4e09\u5c4f\u7684\u661f\u661f\r\n      function star(){\r\n    \t  var windowWidth = document.body.offsetWidth;\r\n\t\t  var windowHeight = document.body.offsetHeight;\r\n          var starLeft = Math.random()*windowWidth;\r\n          var starTop = Math.random()*windowHeight;\r\n    \t  \r\n          \/\/\u521b\u5efa\u661f\u661fdiv\r\n          var starDiv = document.createElement(\"div\");\r\n          starDiv.innerHTML = \"<img src='http:\/\/www.youyu001.com\/res\/htmlclassics\/full\/images\/star.png' \/>\";\r\n          starDiv.style.position = \"absolute\";\r\n          starDiv.style.top = starTop+\"px\";\r\n          starDiv.style.left = starLeft+\"px\";\r\n          starDiv.style.opacity = 0;\r\n          starDiv.className = \"starAnimation\";\r\n          \/\/\u5c06\u96ea\u82b1\u6dfb\u52a0\u5230body\r\n          document.getElementById(\"page2\").appendChild(starDiv);\r\n          \r\n          starDiv.addEventListener(\"animationend\",function(){\r\n        \t  document.getElementById(\"page2\").removeChild(starDiv);\r\n          });\r\n          \r\n          t1 = window.setTimeout(\"star()\",50);\r\n      }\r\n    <\/script>\r\n  <\/head>\r\n  <body ontouchmove=\"return false;\">\r\n    <!-- \u7f51\u7ad9\u5934 -->\r\n    <div class=\"head\">\r\n      <ul>\r\n        <li><a href=\"#\">\u7f51\u7ad9\u9996\u9875<\/a><\/li>\r\n        <li><a href=\"#\">\u5728\u7ebf\u8bfe\u5802<\/a><\/li>\r\