<script>

//微信二维码绑定用户
function wxlogin(){
	swal({
		title: '学校账号已过期!',
		text:"您可以使用微信扫码继续学习，系统将同步您的学习记录",
		type: 'question',
		confirmButtonColor: '#3085d6',
		confirmButtonText: '确定',
		showCloseButton: false,
		showCancelButton: false
	}).then(function(isConfirm) {
		if(isConfirm) {
			// var url = "https://open.weixin.qq.com/connect/qrconnect?appid=wxb0359ad17f776ae3&redirect_uri=http%3a%2f%2fwww.yyfun001.com%2flesson%2faction.php%3fc%3dUserProxy%26a%3dnewUserScanQRCodeByTOB&response_type=code&scope=snsapi_login&state=STATE#wechat_redirect";
			// var el = document.createElement("a");
			// document.body.appendChild(el);
			// el.href = url; //url 是你得到的连接
			// el.target = '_self'; //指定在新窗口打开
			// el.click();
			// document.body.removeChild(el);
		}
	});

	$(".swal2-modal").css({"width":"600px"});
	$(".swal2-content").show();


	//登录二维码
	try{
		var wxlogin = new WxLogin({
			id: "swal2-content",
			appid: "wx97cc371bfbefad99",
			scope: "snsapi_login",
			redirect_uri: encodeURIComponent("http://www.youyu001.com/lesson/action.php?c=UserProxy&a=newUserScanQRCodeByTOB"),
			state: Math.ceil(Math.random()*1000),
			style: "black",
			href: "data:text/css;base64,DQoJCQkJCS5pbXBvd2VyQm94IC5xcmNvZGUge3dpZHRoOiA3MCU7fQ0KCQkJCQkuaW1wb3dlckJveCAuaW5mbyB7d2lkdGg6ICAxM3JlbTt9DQoJCQkJCS5pbXBvd2VyQm94IC50aXRsZSB7ZGlzcGxheTogbm9uZTt9DQoJCQkJCS5zdGF0dXNfaWNvbiB7ZGlzcGxheTpub25lfQ0KCQkJCQkuaW1wb3dlckJveCAuc3RhdHVzIHt0ZXh0LWFsaWduOiBjZW50ZXI7fQ0KCQkJCQk="
			});
	}catch(e){}
}

//退出登录
function outLogin(){
	swal({
		text:"确定退出登录吗?",
		type: 'question',
		showCancelButton: true, 
		confirmButtonColor: "#ff6600",
		confirmButtonText: "确定",
		cancelButtonColor: "#d2d2d2", 
		cancelButtonText: "取消",
		closeOnConfirm: false, 
		closeOnCancel: true
	}).then(function(isConfirm) {
		if(isConfirm) {
			window.location.href="login.php?out=1";
		}
	});
}

//监听滚动条，设置导航栏
window.addEventListener('scroll', function(){ 

	let t = $('body, html').scrollTop(); 

	if(t > 0){ 
		$('#nav-header').addClass('box-active') 
	}else{ 
		$('#nav-header').removeClass('box-active') 
	}

});


