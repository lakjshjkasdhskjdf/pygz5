r\n    \t  \/\/\u94a2\u7434\u5757\uff0c\u5173\u95ed\u6309\u626d\r\n    \t  var closeBtnList = document.getElementsByClassName(\"closeBtn\");\r\n    \t  for(var i=0;i<closeBtnList.length;i++){\r\n\t    \t  closeBtnList[i].onclick = function(e){\r\n\t    \t\t  e.stopPropagation();\/*\u963b\u6b62\u4e8b\u4ef6\u5192\u6ce1(\u70b9\u51fb\u5173\u95ed\u6309\u626d\u65f6\uff0cli\u548cul\u4e5f\u4f1a\u88ab\u70b9\u51fb\u5230\u3002\u8981\u963b\u65ad\u8fd9\u6548\u679c\uff0c\u5fc5\u987b\u7528\u8fd9\u53e5\u4ee3\u7801)*\/\r\n\t    \t\t  document.getElementsByClassName(\"active\")[0].className = \"\";\r\n\t    \t\t  document.getElementById(\"page2_ul\").className = \"\";\r\n\t    \t  }\r\n      \t  }\r\n      }\r\n      \r\n      \/\/\u6eda\u8f6e\u4e8b\u4ef6\u8c03\u7528\u51fd\u6570\r\n      function mousewheel(e){\r\n\t    if(!isScroll){\r\n\t    \tisScroll = true;\r\n\t\t    e.preventDefault();\/\/\u901a\u77e5\u6d4f\u89c8\u5668\u4e0d\u6267\u884c\u9ed8\u8ba4\u7684\u52a8\u4f5c\r\n\t\t    \r\n\t\t    if(e.wheelDelta){\r\n\t\t    \t\/\/IE\/Opera\/Chrome\r\n\t\t        if(e.wheelDelta < 0){\r\n\t\t        \tindex++;\r\n\t\t        }\r\n\t\t        else{\r\n\t\t        \tindex--;\r\n\t\t        }\r\n\t\t    }\r\n\t\t    else if(e.detail){\r\n\t\t    \t\/\/Firefox\r\n\t\t        if(e.detail < 0){\r\n\t\t        \tindex--;\r\n\t\t        }\r\n\t\t        else{\r\n\t\t        \tindex++;\r\n\t\t        }\r\n\t\t    }\r\n\t\t    setPageAnimate()\r\n\t    }\r\n\t  }\r\n      \r\n      \/\/\u8bbe\u7f6e\u6bcf\u5c4f\u5165\u573a\u52a8\u753b\r\n      function setPageAnimate(){\r\n    \t  if(index < 0){\r\n    \t\t  index = 0;\r\n\t    \t  isScroll = false;\r\n\t      }\r\n\t      if(index > maxIndex){\r\n\t    \t  index = maxIndex;\r\n\t    \t  isScroll = false;\r\n\t      }\r\n\t      \r\n\t      \/\/\u8bbe\u7f6e\u53f3\u4fa7\u7684\u6d6e\u52a8\u6309\u626d\r\n\t\t  for(var i=0;i<=maxIndex;i++){\r\n\t\t\t  document.getElementById(\"_btn\"+i).style.backgroundColor = \"#3D3D3D\";\r\n\t\t  }\r\n\t\t  docume