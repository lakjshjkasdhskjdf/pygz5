          document.getElementById(\"mainDiv\").style.top = top+\"px\";\r\n          \r\n          isScroll = false;\r\n      }\r\n      \r\n      \/\/\u7b2c\u4e8c\u5c4f\uff0c\u663e\u793a\u89c6\u9891\r\n      function showVideo(){\r\n    \t  document.getElementById(\"page1_3\").style.display = \"block\";\r\n    \t  document.getElementById(\"vo\").play();\r\n    \t  \/\/document.getElementById(\"vo\").pause();\/\/\u505c\u6b62\r\n      }\r\n    <\/script>\r\n  <\/head>\r\n  <body ontouchmove=\"return false;\">\r\n    <!-- \u7f51\u7ad9\u5934 -->\r\n    <div class=\"head\">\r\n      <ul>\r\n        <li><a href=\"#\">\u7f51\u7ad9\u9996\u9875<\/a><\/li>\r\n        <li><a href=\"#\">\u5728\u7ebf\u8bfe\u5802<\/a><\/li>\r\n        <li><a href=\"#\">\u4ed8\u8d39\u8bfe\u7a0b<\/a><\/li>\r\n        <li><a href=\"#\">\u5168\u7ad9\u641c\u7d22<\/a><\/li>\r\n      <\/ul>\r\n    <\/div>\r\n    \r\n    <!-- \u6b63\u6587\u5185\u5bb9 -->\r\n    <div id=\"mainDiv\">\r\n      \r\n      <!-- \u7b2c\u4e00\u5c4f -->\r\n      <div id=\"page0\" class=\"enter\">\r\n        <div id=\"page0_1\"><img src=\"http:\/\/www.youyu001.com\/res\/htmlclassics\/full\/images\/page1_image.png\" \/><\/div>\r\n        <div id=\"page0_2\">\r\n          <h1>\u590d\u4ec7\u8005\u8054\u76df<\/h1>\r\n          <p>Marvel's The Avengers \u662f\u4e00\u90e8\u4e8e2012\u5e74\u4e0a\u6620\u7684\u8d85\u7ea7\u82f1\u96c4\u7535\u5f71\uff0c\u6539\u7f16\u81ea\u540c\u540d\u6f2b\u753b\uff0c\u7531\u4e54\u65af.\u60e0\u767b\u6267\u5bfc\u3002\u672c\u7247\u662f\u6f2b\u5a01\u6539\u7f16\u7684\u7535\u5f71\u4e2d\u7b2c\u4e00\u90e8\u83b7\u5f97\u8d85\u8fc7\u5341\u4ebf\u7968\u623f\u7684\u7535\u5f71\u3002<\/p>\r\n          <p>\u540c\u65f6\u4e5f\u662f\u4e16\u754c\u7535\u5f71\u53f2\u4e0a\u7968\u623f\u6536\u5165\u7b2c\u516d\u9ad8\u7684\u7535\u5f71\uff08\u6b21\u4e8e\u300a\u963f\u51e1\u8fbe\u300b\u3001\u300a\u6cf0\u5766\u5c3c\u514b\u53f7\u300b\u3001\u300a\u661f\u7403\u5927\u6218\uff1a\u539f\u529b\u89c9\u9192\u300b\u3001\u300a\u4f8f\u7f57\u7eaa\u4e16\u7