}\r\n\t      if(index > maxIndex){\r\n\t    \t  index = maxIndex;\r\n\t    \t  isScroll = false;\r\n\t      }\r\n\t      \r\n\t      \/\/\u8bbe\u7f6e\u53f3\u4fa7\u7684\u6d6e\u52a8\u6309\u626d\r\n\t\t  for(var i=0;i<=maxIndex;i++){\r\n\t\t\t  document.getElementById(\"_btn\"+i).style.backgroundColor = \"#3D3D3D\";\r\n\t\t  }\r\n\t\t  document.getElementById(\"_btn\"+index).style.backgroundColor = \"#10508D\";\r\n\t\t  \r\n\t\t  \/\/\u91cd\u7f6e\u7b2c\u4e8c\u5c4f\u7684\u89c6\u9891\r\n\t      if(index != 1){\r\n\t    \t  document.getElementById(\"vo\").pause();\r\n\t    \t  document.getElementById(\"vo\").currentTime = 0;\/\/\u56de\u5230\u5f00\u5934\r\n\t    \t  document.getElementById(\"page1_3\").style.display = \"none\";\r\n\t      }\r\n\t\t  \r\n\t      turnPage();\r\n      }\r\n      \r\n      \/\/\u6eda\u5c4f\u52a8\u753b\r\n      function turnPage(){  \r\n    \t  \r\n          var top = document.body.offsetHeight * index * -1;\r\n          document.getElementById(\"mainDiv\").style.top = top+\"px\";\r\n          \r\n          isScroll = false;\r\n      }\r\n      \r\n      \/\/\u7b2c\u4e8c\u5c4f\uff0c\u663e\u793a\u89c6\u9891\r\n      function showVideo(){\r\n    \t  document.getElementById(\"page1_3\").style.display = \"block\";\r\n    \t  document.getElementById(\"vo\").play();\r\n    \t  \/\/document.getElementById(\"vo\").pause();\/\/\u505c\u6b62\r\n      }\r\n    <\/script>\r\n  <\/head>\r\n  <body ontouchmove=\"return false;\">\r\n    <!-- \u7f51\u7ad9\u5934 -->\r\n    <div class=\"head\">\r\n      <ul>\r\n        <li><a href=\"#\">\u7f51\u7ad9\u9996\u9875<\/a><\/li>\r\n        <li><a href=\"#\">\u5728\u7ebf\u8bfe\u5802<\/a><\/li>\r\n        <li><a href=\"#\">\u4ed8\u8d39\u8bfe\u7a0b<\/a><\/li>\r\n        <li><a href=\"#\">\u5168\u7ad9\u641c\u7d22<\/a><\/li>\r\n      <\/ul>\r\n    <\/div>\r\n    \r\n    <!-- \u6b63\u6587\u5185\u5bb9 -->\r\n    <div id=\"mainDiv\">\r\n      \r\n      <!-- \u7b2c\u4e00\u5c4f -->\r\n      <div id=\"page0\" class=\"enter\">\r\n 